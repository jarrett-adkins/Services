<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.services.MainActivity">

    <Button
        android:id="@+id/btnStartedService"
        android:text="Started Service"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnStopService"
        android:text="Stop Service"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnStartIntentService"
        android:text="Start Intent Service"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnStartIntentService2"
        android:text="Start Intent Service2"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnBoundToService"
        android:text="Bound to car Service"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnBoundGetCar"
        android:text="Get Car List From Bound"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnBoundAddCar"
        android:text="Add Car to Bound Service"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnUnbind"
        android:text="Unbind Car Service"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />>

    <Button
        android:id="@+id/btnScheduleJob"
        android:text="Schedule a Job"
        android:onClick="startServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>
